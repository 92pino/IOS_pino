# platform :ios, '9.0'

target 'FirebaseDatabase' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FirebaseDatabase
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Analytics'
  pod 'Firebase/RemoteConfig'
  pod 'SnapKit', '~> 5.0.0'

end
